  "triggers": [
    {
      "id": "time-based",
      "name": "时间触发器",
      "rules": [
        {
          "condition": "hour >= 6 && hour < 12",
          "preset": "energetic",
          "combo": "morning-routine"
        },
        {
          "condition": "hour >= 12 && hour < 18",
          "preset": "professional",
          "combo": null
        },
        {
          "condition": "hour >= 18 && hour < 22",
          "preset": "calm",
          "combo": null
        },
        {
          "condition": "hour >= 22 || hour < 6",
          "preset": "calm",
          "combo": "goodnight"
        }
      ]
    },
    {
      "id": "interaction-based",
      "name": "交互触发器",
      "rules": [
        {
          "condition": "idle_time > 300",
          "animation": "sleep",
          "message": "主人去哪了？"
        },
        {
          "condition": "click_count > 5",
          "animation": "excited",
          "message": "好开心！"
        },
        {
          "condition": "drag_duration > 10",
          "animation": "wiggle",
          "message": "好晕啊~"
        }
      ]
    },
    {
      "id": "status-based",
      "name": "状态触发器",
      "rules": [
        {
          "condition": "cpu_usage > 80",
          "animation": "working",
          "message": "电脑好累..."
        },
        {
          "condition": "battery < 20",
          "animation": "sleep",
          "message": "快没电了"
        },
        {
          "condition": "notification_received",
          "animation": "excited",
          "combo": "attention-seeking"
        }
      ]
    }
  ],
